<!-- 第一部分 -->
<swiper class="swiper-banner" indicator-dots="{{indicatorDots}}" circular="true" previous-margin="35px"
next-margin="35px"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block wx:for="{{imgPath}}" wx:key="*this">
          <swiper-item>
            <!-- <view  class=" box">swiper-item {{item}}</view> -->
            <image src="{{item}}"></image>
          </swiper-item>
        </block>
</swiper>

<!-- end 第一部分 -->

