<!-- 第四部分 -->
<view class="goodCook width96">
<view class="goodCook-top">
  <image src="../img/9.png"></image>
</view>
<view class="goodCook-middle">
  <view wx:for="{{cookArr}}" class="goodCook-item" wx:key="xx">
  <image class="goodCook-middle-img1" src="{{item.imgPath}}" ></image>
  <view class="goodCook-name">{{item.name}}</view>
  <view class="star">
    <image class="goodCook-middle-img2" src="../img/star.png" style="margin-top:-116px;" wx:if="{{item.star == 5}}"></image>
    <image class="goodCook-middle-img2" src="../img/star.png" style="margin-top:-94px;" wx:if="{{item.star == 4}}"></image>
    <image class="goodCook-middle-img2" src="../img/star.png" style="margin-top:-74px;" wx:if="{{item.star == 3}}"></image>
    <image class="goodCook-middle-img2" src="../img/star.png" style="margin-top:-52px;" wx:if="{{item.star == 2}}"></image>
    <image class="goodCook-middle-img2" src="../img/star.png" style="margin-top:-31px;" wx:if="{{item.star == 1}}"></image>
    <image class="goodCook-middle-img2" src="../img/star.png" style="margin-top:-10px;" wx:if="{{item.star == 0}}"></image>
  </view>
  </view>
</view>
<view class="goodCook-bottom"></view>

</view>
<!-- end 第四部分 -->

<!-- 第五部分 -->
<view class="superDinner">
<view class="dinnerTop">
  <image class="dinnerImg1" src="../img/10.png"></image>
</view>
  <scroll-view scroll-x="true" class="dinnerBox" >
  <view class="dinnerBoxIn">
    <view class="dinnerItem" wx:for="{{dinnerArr}}" wx:key="xx">
      <image src="{{item.imgPath}}"></image>
      <view class="dinnerName">{{item.name}}</view>
      <view class="dinnerPrice">{{item.price}}</view>
    </view> 
  </view>
  </scroll-view>
</view>

